<?xml version="1.0" encoding="utf-8"?>
<resources>
    <integer-array name="spb_colors">
        <item>@color/logo_color_blue</item>
        <item>@color/logo_color_orange</item>
        <item>@color/logo_color_green</item>
    </integer-array>
    <color name="default_text_color_dark">#6c6c6c</color>
    <color name="default_text_color_light">#f0f0ee</color>
    <color name="logo_color_blue">#3399ff</color>
    <color name="logo_color_green">#99cc33</color>
    <color name="logo_color_orange">#ff9933</color>
    <color name="to_confirm_paper_color">#f4f4f4</color>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_inapp_browser_width">300dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <string name="confirm_iap_cancel">Are you sure you want to cancel?</string>
    <string name="error_generic_try_after_st">Sorry, we had an error. Please try after some time.</string>
    <string name="label_cancel">Cancel</string>
    <string name="label_no">No</string>
    <string name="label_ok">OK</string>
    <string name="label_pl_wait_goingback">Please wait.. Reloading last page..</string>
    <string name="label_pl_wait_reloading">Please wait.. Reloading current page..</string>
    <string name="label_try_again">Try Again</string>
    <string name="label_yes">Yes</string>
    <string name="lable_please_wait">please wait</string>
    <string name="msg_internet_issue">We don\'t look to have active internet connection!</string>
    <string name="msg_servers_issue">Unable to reach our servers!\n\nPlease check connection.</string>
    <string name="start_qco_call">Pay with Wibmo</string>
    <string name="title_sdk_inapp">Wibmo IAP</string>
    <string name="wibmosdk_phone_state_permission_missing_msg">You need to allow access to basic info about your phone to secure this transaction.</string>
    <string name="wibmosdk_phone_state_permission_rationale">We need access to basic info about your phone to secure your transaction.</string>
    <style name="Wibmo.Transparent" parent="android:Theme">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="WibmoConfirmTitle" parent="WibmoFontHeading">
        <item name="android:textColor">@color/default_text_color_dark</item>
        <item name="android:textSize">12sp</item>
    </style>
    <style name="WibmoFont" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/default_text_color_dark</item>
        <!-- item name="android:typeface">monospace</item -->
        <item name="android:typeface">sans</item>
        <!-- item name="android:typeface">monospace</item -->
        <item name="android:textSize">25sp</item>
    </style>
    <style name="WibmoFontHeading" parent="WibmoFont">
        <item name="android:textColor">@color/default_text_color_dark</item>
        <item name="android:textSize">17sp</item>
    </style>
    <style name="WibmoFontLabel" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#6c6c6c</item>
        <!-- item name="android:typeface">monospace</item -->
        <item name="android:typeface">sans</item>
        <!-- item name="android:typeface">monospace</item -->
        <item name="android:textSize">15sp</item>
    </style>
    <style name="WibmoIAPWait" parent="WibmoFont">
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">@color/logo_color_blue</item>
    </style>
</resources>